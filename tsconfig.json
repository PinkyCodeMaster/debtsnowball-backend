{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "strict": true,
    "outDir": "./dist",
    "skipLibCheck": true,
    "typeRoots": [
      "./node_modules/@types"
    ]
  }
}